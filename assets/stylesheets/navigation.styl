nav
  width 100%
  height 85px
  background #2c353a

  ul li
    padding 20px
    float left

    a
      text-align center
      color #848e92
      display block
      padding 0 30px
      font-weight 700

      span
        display block

      &:hover, &.active
        color #fff
        text-decoration none

    &:hover, &.active
      cursor pointer
      background #222b2f
