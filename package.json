  "scripts": {
    "start": "webpack --config config/webpack.server.js --mode=development && node ./dist/server.js",
    "start:staging": "API_URL=https://lego-staging.abakus.no/api/v1 BASE_URL=https://lego-staging.abakus.no WS_URL=wss://ws-staging.abakus.no CAPTCHA_KEY=6LfS1TQUAAAAAIcxgj4EfkM7lwWeG9efLvS37tCW ENVIRONMENT=local_staging yarn start",
    "ssr:staging": "API_URL=https://lego-staging.abakus.no/api/v1 BASE_URL=https://lego-staging.abakus.no WS_URL=wss://ws-staging.abakus.no CAPTCHA_KEY=6LfS1TQUAAAAAIcxgj4EfkM7lwWeG9efLvS37tCW yarn ssr",
    "ssr": "echo \"Remember that SSR does not have hot reloading, build with \"yarn build\"\" && node ./dist/server.js",
    "build": "concurrently \"yarn run build:server\" \"yarn run build:client\"",
    "build:all": "concurrently \"yarn build:server\" \"yarn build:client\" \"yarn build:dll\"",
    "build:client": "webpack --config config/webpack.client.js --mode=production",
    "build:server": "webpack --config config/webpack.server.js --mode=production",
    "build:dll": "BUILDING_DLL=true webpack --color --config config/webpack.dll.js",
    "postinstall": "yarn run build:dll",
    "test": "NODE_ENV=test jest",
    "test:coverage": "yarn run test -- --coverage",
    "test:watch": "yarn run test --watch",
    "lint": "yarn run lint:js && yarn run lint:css && yarn run lint:prettier",
    "lint:js": "eslint . --ignore-path .prettierignore --max-warnings 0",
    "lint:css": "stylelint './app/**/*.css'",
    "lint:prettier": "prettier '**/*.{js,css,md}' --check",
    "prettier": "prettier '**/*.{js,css,md}' --write",
    "flow": "flow"
  },
