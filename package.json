{
  "name": "lego-webapp-monorepo",
  "version": "1.0.0",
  "description": "Abakus",
  "main": "index.js",
  "author": "webkom",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm run --filter lego-webapp dev",
    "dev:staging": "pnpm run --filter lego-webapp dev:staging",
    "start": "pnpm run --filter lego-webapp start",
    "start:staging": "pnpm run --filter lego-webapp start:staging",
    "build": "pnpm run -r build",
    "preview": "pnpm run --filter lego-webapp preview",
    "preview:staging": "pnpm run --filter lego-webapp preview:staging",
    "ssr": "pnpm run --filter lego-webapp ssr",
    "ssr:staging": "pnpm run --filter lego-webapp ssr:staging",
    "test": "pnpm run -r test",
    "lint": "pnpm run -r lint",
    "prettier": "pnpm run -r prettier",
    "types": "pnpm run -r types"
  },
  "repository": {
    "type": "git",
    "url": "github.com/webkom/lego-webapp"
  },
  "dependencies": {
    "prettier": "^3.5.3"
  },
  "prettier": {
    "singleQuote": true
  },
  "packageManager": "pnpm@10.1.0"
}
