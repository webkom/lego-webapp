    "webpack-hot-middleware": "^2.24.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-stats-plugin": "^0.3.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": null,
      "string-quotes": "single",
      "indentation": 2,
      "custom-property-empty-line-before": null,
      "declaration-colon-newline-after": null,
      "property-no-unknown": null,
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "jest": {
    "globals": {
      "__DEV__": false,
      "__CLIENT__": true
    },
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp3|mp4|webm)$": "<rootDir>/testing/FileStub.js",
      "^.+\\.css$": "<rootDir>/testing/CSSStub.js"
    },
    "testRegex": "\\.spec\\.(js|jsx)$",
    "setupFiles": [
      "<rootDir>/testing/requestAnimationFrame.js",
      "<rootDir>/testing/setup.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "dllPlugin": {
    "path": "dist-client",
    "exclude": [
      "animate.css",
      "minireset.css",
      "jsdom",
      "bunyan",
      "bunyan-pretty",
      "@sentry/node",
      "source-map-support",
      "convert-source-map"
    ]
  },
  "moduleRoots": [
    "./"
  ]
}
